api_key: ""

enable_metrics_collecting: false
metrics_host: influxdb:8086
metrics_database: steamtracker-metrics

enable_profile_updates: true
profile_update_cron: "0 0 */12 * * *"
profile_update_batch_size: 100

enable_snapshot_updates: true
snapshot_update_cron: "0 0 3,15 * * *"
snapshot_update_batch_size: 10